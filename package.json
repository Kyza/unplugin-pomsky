		"typescript": "^4.9.5",
		"vite": "^4.1.2",
		"webpack": "^5.75.0"
	},
	"dependencies": {
		"d-forest": "^3.2.4",
		"estree-walker": "^3.0.3",
		"magic-string": "^0.30.0",
		"pomsky-wasm": "^0.9.2",
		"unplugin": "^1.1.0"
	},
	"files": [
		"package.json",
		"dist",
		"LICENSE",
		"tsconfig.json",
		"types.d.ts",
		"files.d.ts"
